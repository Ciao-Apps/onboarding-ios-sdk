{
  "flowID": "banking_onboarding_v1",
  "appID": "secure_bank_app",
  "version": "1.0",
  "pages": [
    {
      "id": "welcome",
      "type": "text_image",
      "title": "Welcome to SecureBank",
      "subtitle": "Banking made simple and secure",
      "imageURL": "https://images.unsplash.com/photo-1556742502-ec7c0e9f34b1?w=400&h=300&fit=crop",
      "style": {
        "titleColor": "#1D1D1F",
        "subtitleColor": "#6E6E73"
      }
    },
    {
      "id": "account_type",
      "type": "selector",
      "title": "What type of account do you need?",
      "subtitle": "Choose the account that fits your needs",
      "key": "account_type",
      "options": ["Personal Checking", "Business Checking", "Savings Account", "Investment Account"]
    },
    {
      "id": "monthly_income",
      "type": "slider",
      "title": "What's your monthly income range?",
      "subtitle": "This helps us recommend the right services",
      "key": "monthly_income",
      "min": 0,
      "max": 20000,
      "step": 1000
    },
    {
      "id": "phone_number",
      "type": "input",
      "title": "Enter your phone number",
      "subtitle": "We'll use this for account verification",
      "placeholder": "+1 (555) 123-4567",
      "inputType": "number",
      "key": "phone"
    },
    {
      "id": "security_preference",
      "type": "selector",
      "title": "Preferred security method",
      "subtitle": "How would you like to secure your account?",
      "key": "security_method",
      "options": ["SMS Verification", "Email Verification", "Authenticator App", "Biometric (Face ID/Touch ID)"]
    },
    {
      "id": "completion",
      "type": "text_image",
      "title": "Account Setup Complete",
      "subtitle": "You're ready to start banking with us!",
      "imageURL": "https://images.unsplash.com/photo-1551288049-bebda4e38f71?w=400&h=300&fit=crop",
      "button": {
        "title": "Open My Account",
        "action": "finish",
        "style": {
          "backgroundColor": "#34C759",
          "textColor": "#FFFFFF",
          "cornerRadius": 25
        }
      }
    }
  ]
}
